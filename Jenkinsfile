pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        sh '''
                echo \'========= Checkout stage ==========\'




                
            '''
      }
    }
    stage('Build') {
      steps {
        sh ' echo \'========= Build stage ==========\''
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo \'========= Deploy stage ==========\''
      }
    }
  }
}